                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Orders.Id, OrderDetail.Quantity, OrderDetail.Discount, Customer.Name, Customer.Address, Customer.Nif, Payment.Amount as PaymentAmount, Payment.PaymentMethod, Orders.Pending, Orders.Total, Orders.SubTotal, Orders.Remarks, Orders.Status, Orders.Date, 

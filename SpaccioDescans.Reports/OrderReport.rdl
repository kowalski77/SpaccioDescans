  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>OrdersDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrderId">
            <Value>=Parameters!OrderId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Customer.Name, Customer.Address, Customer.Nif, OrderDetail.Quantity, OrderDetail.Discount, Orders.Id, Orders.Date, Orders.Status, Orders.Remarks, Orders.SubTotal, Orders.Total, Orders.Pending, Payment.PaymentMethod, 
                         Payment.Amount, Products.Vendor, Products.Name AS ProductName, Products.Description, Products.NetPrice, Products.Measures
FROM            Customer INNER JOIN
                         OrderDetail ON Customer.Id = OrderDetail.Id INNER JOIN
                         Orders ON Customer.Id = Orders.CustomerId AND OrderDetail.OrderId = Orders.Id INNER JOIN
                         Payment ON Orders.Id = Payment.OrderId INNER JOIN
                         Products ON OrderDetail.ProductId = Products.Id
WHERE Orders.Id = @OrderId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

  <DataSets>
    <DataSet Name="OrdersDataSet">
      <Query>
        <DataSourceName>SpaccioDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@oId">
            <Value>=Parameters!oId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Customer.Name, Customer.Address, Customer.Nif, Customer.Phone, OrderDetail.Quantity, OrderDetail.Discount, Orders.Date, Orders.Status, Orders.Remarks, Orders.SubTotal, Orders.Total, Orders.Pending, Payment.Amount, 
                         Payment.PaymentMethod, Products.Vendor, Products.Name AS ProductName, Products.Description, Products.Measures, Products.NetPrice, Orders.Id
FROM            Customer INNER JOIN
                         OrderDetail ON Customer.Id = OrderDetail.Id INNER JOIN
                         Orders ON Customer.Id = Orders.CustomerId AND OrderDetail.OrderId = Orders.Id INNER JOIN
                         Payment ON Orders.Id = Payment.OrderId INNER JOIN
                         Products ON OrderDetail.ProductId = Products.Id
WHERE        (Orders.Id = @oId)</CommandText>
      </Query>
